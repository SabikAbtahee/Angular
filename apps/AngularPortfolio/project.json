{
	"name": "AngularPortfolio",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"projectType": "application",
	"generators": {
		"@schematics/angular:component": {
			"style": "scss"
		}
	},
	"sourceRoot": "apps/AngularPortfolio/src",
	"prefix": "app",
	"targets": {
		"build": {
			"executor": "@angular-devkit/build-angular:browser",
			"options": {
				"outputPath": "dist/apps/AngularPortfolio",
				"index": "apps/AngularPortfolio/src/index.html",
				"main": "apps/AngularPortfolio/src/main.ts",
				"polyfills": ["zone.js"],
				"tsConfig": "apps/AngularPortfolio/tsconfig.app.json",
				"inlineStyleLanguage": "scss",
				"assets": ["apps/AngularPortfolio/src/assets"],
				"styles": [
					"apps/AngularPortfolio/src/styles/custom-theme.scss",
					"apps/AngularPortfolio/src/styles/styles.scss"
				],
				"scripts": []
			},
			"configurations": {
				"production": {
					"budgets": [
						{
							"type": "initial",
							"maximumWarning": "500kb",
							"maximumError": "1mb"
						},
						{
							"type": "anyComponentStyle",
							"maximumWarning": "2kb",
							"maximumError": "4kb"
						}
					],
					"outputHashing": "all"
				},
				"development": {
					"buildOptimizer": false,
					"optimization": false,
					"vendorChunk": true,
					"extractLicenses": false,
					"sourceMap": true,
					"namedChunks": true
				}
			},
			"defaultConfiguration": "production"
		},
		"serve": {
			"executor": "@angular-devkit/build-angular:dev-server",
			"configurations": {
				"production": {
					"browserTarget": "AngularPortfolio:build:production"
				},
				"development": {
					"browserTarget": "AngularPortfolio:build:development"
				}
			},
			"defaultConfiguration": "development"
		},
		"extract-i18n": {
			"executor": "@angular-devkit/build-angular:extract-i18n",
			"options": {
				"browserTarget": "AngularPortfolio:build"
			}
		},
		"test": {
			"executor": "@angular-devkit/build-angular:karma",
			"options": {
				"polyfills": ["zone.js", "zone.js/testing"],
				"tsConfig": "apps/AngularPortfolio/tsconfig.spec.json",
				"inlineStyleLanguage": "scss",
				"assets": ["apps/AngularPortfolio/src/assets"],
				"styles": ["apps/AngularPortfolio/src/styles/styles.scss"],
				"scripts": []
			}
		},
		"lint": {
			"executor": "@nx/linter:eslint",
			"options": {
				"lintFilePatterns": [
					"apps/AngularPortfolio/**/*.ts",
					"apps/AngularPortfolio/**/*.html"
				]
			}
		}
	}
}
